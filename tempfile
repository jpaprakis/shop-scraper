	gtmECommerce.currentIndex = gtmECommerce.currentIndex || 0;
	gtmECommerce.history = gtmECommerce.history || {};

	// Prevent app.init() ==&gt; ... ==&gt; loadRecommendations() from running the js in $("#carousel-recomendations") again
	if (!(app.product &amp;&amp; app.product.recommendationsLoaded)) {
		
		var product = {
			'name':			'Printed short sleeve sweater',	// Required.
	        'id':			'743651',	// Required.
	        //'dimension16':	'743651',
	        //'metric2':		'55.9',
	        //'metric3':		'39.95',
	        'price':		'39.95',
	        'brand':		'RW&amp;CO',
	        'category':		'Tops',
	        'variant':		'Off White',	// will use the color as a variant (if applicable)
	        'list':			gtmECommerce.listRelated || gtmECommerce.list || 'Search Results',
	        'position':		++gtmECommerce.currentIndex	// Position in the grid or list
		};

		gtmECommerce.data.ecommerce.impressions.push(product);
		gtmECommerce.history[product.id] = product;
	}
</script>

	
	
	


				
			
