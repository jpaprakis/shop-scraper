				app.gtm.click.pushEvent(eventCategory11, 'Use This Address For Billing - Checked', orderTotal);
			} else {
				app.gtm.click.pushEvent(eventCategory11, 'Use This Address For Billing - unchecked', orderTotal);
			}
	});
	
	
	
	
